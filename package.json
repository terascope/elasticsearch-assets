    "bugs": {
        "url": "https://github.com/jsnoble/elasticsearch-assets/issues"
    },
    "homepage": "https://github.com/jsnoble/elasticsearch-assets#readme",
    "dependencies": {},
    "devDependencies": {
        "@terascope/job-components": "^0.20.8",
        "@terascope/teraslice-op-test-harness": "~1.7.1",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.0.0",
        "eslint-plugin-import": "^2.18.2",
        "jest": "^24.8.0",
        "jest-extended": "^0.11.2",
        "nock": "^10.0.6",
        "teraslice-test-harness": "^0.8.2"
    }
